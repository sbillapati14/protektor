{
  "name": "@iris-platform/protektor",
  "version": "1.0.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test jest --coverage --config ./jest.config.js",
    "test:watch": "jest --watch --config ./jest.config.js",
    "lint": "eslint -c .eslintrc src",
    "prebuild": "npm run lint && rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist src",
    "build:umd": "webpack --mode production --output-filename protektor.js",
    "build:umd.min": "webpack --mode production --output-filename protektor.min.js -p"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "keywords": [],
  "author": "Rob Sliwa",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.6.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "jest": "^23.2.0",
    "npm-run-all": "^4.1.3",
    "regenerator-runtime": "^0.12.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.14.0",
    "webpack-command": "^0.3.1"
  }
